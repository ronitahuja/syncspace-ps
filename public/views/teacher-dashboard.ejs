  </aside><!-- End Sidebar-->

  <main id="main" class="main">

    <div class="pagetitle">
      <h1>Dashboard</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/teacher/<%=teachername1 %>">Home</a></li>
          <li class="breadcrumb-item active">Dashboard</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section dashboard">
      <div class="row">

        <!-- Left side columns -->
        <div class="col-lg-8">
          <div class="row">
            <table>
              <thead>
                <tr>
                  <th>Documents Uploaded</th>
                  <th>Delete Document</th>
                </tr>
              </thead>
              <% if (rows.length>0){ %>
                <% rows.forEach(row=>{ %>
                  <tr>
                    <td>
                      <img src="/uploads/<%= row.image %>" alt="image" height="150" width="200">
                    </td>
                    <td>
                      <a href="/delete/<%= teachername1 %>/<%= row.image %>" class="btn btn-danger btn-xs">
                        <i class="fa fa-trash"></i>
                      </a>
                    </td>
                  </tr>
                  <% })} %>

            </table>
        
            <form class="col-lg-8" action="/uploads/<%= teachername1 %>" method="post" role="form" enctype="multipart/form-data">
              <input class="col-lg-8" type="file" name="document" multiple >
              <input class="col-lg-8" type="submit" name="submit">
            </form>


          </div>
        </div><!-- End Left side columns -->

      </div>
    </section>

  </main><!-- End #main -->

